CIntervalSymbol::CIntervalSymbol(EIntervalSymbolStyle style)
{
    m_symbol = new QwtIntervalSymbol( QwtIntervalSymbol::Style(style) );
}

/////////////////////////////////////////////////////////////

QwtIntervalSymbol* CIntervalSymbol::ToQwtIntervalSymbol()
{
    return m_symbol;
}

/////////////////////////////////////////////////////////////

CIntervalSymbol::~CIntervalSymbol()
{
    delete [] m_symbol;
}
